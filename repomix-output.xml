          {/* Stylized Quote Icon */}
          <div class="absolute -top-2 -right-2 text-8xl font-serif text-[var(--accent)] opacity-5 select-none pointer-events-none">
            &ldquo;
          </div>
        </div>
      ))}
    </div>
  </div>
</section>
