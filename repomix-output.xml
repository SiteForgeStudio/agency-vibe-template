    {/* MOBILE TOGGLE */}
    <button
      id="menu-toggle"
      class="md:hidden text-[var(--text-primary)] relative z-[130] p-2 focus:outline-none"
      aria-label="Toggle Menu"
      aria-controls="mobile-menu"
      aria-expanded="false"
      type="button"
