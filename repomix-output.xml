    {/* 8.5 SERVICE AREA */}
    {strategy.show_service_area && data.service_area && (
      <ServiceArea service_area={data.service_area} intelligence={intelligence} />
    )}

    {/* 9. FINAL CONVERSION */}
    <Contact contact={contact} intelligence={intelligence} brand={brand} settings={settings} />
  </main>
</Layout>
